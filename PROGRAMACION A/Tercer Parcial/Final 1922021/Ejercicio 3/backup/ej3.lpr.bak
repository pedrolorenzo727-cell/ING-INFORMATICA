program ej3;
Const
     MAXELEM = 20;
Type
    TV= array[1..MAXELEM] of byte;

procedure insertaOrdenado (var A : TV; var n : byte; x : integer);
var
   i : byte;
begin
  i := n;
  while (i > 0) and (x > A[i]) do
  begin
    A[i + 1] := A[i];
    i := i - 1;
  end;
  A[i + 1] := x;
  n := n + 1;
end;

procedure leerVector (var A : TV; var n : byte);
